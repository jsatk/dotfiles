account earnest
host smtp.gmail.com
port 587
protocol smtp
auth on
from jesse.atkinson@earnest.com
user jesse.atkinson@earnest.com
tls on
tls_trust_file ~/.mutt/Equifax_Secure_CA.cert

account jsatk
host smtp.fastmail.com
port 465
protocol smtp
auth plain
from jesse@jsatk.us
user jesse@jsatk.us
tls on
tls_starttls off
tls_trust_file ~/.mutt/DigiCertGlobalRootCA.crt

account default : jsatk
