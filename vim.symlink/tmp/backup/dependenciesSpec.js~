describe('Dependencies', function() {
  it('should have loaded Handlebars', function() {
    expect(Handlebars).to.exist;
    expect(Handlebars).not.to.be.undefined;
  });

  it('should have loaded jQuery', function() {
    expect(jQuery).to.exist;
    expect(jQuery).not.to.be.undefined;
    expect($).to.exist;
    expect($).not.to.be.undefined;
  });

  it('should have loaded Backbone', function() {
    expect(Backbone).to.exist;
    expect(Backbone).not.to.be.undefined;
  });

  it('should have loaded Marionette', function() {
    expect(Marionette).to.exist;
    expect(Marionette).not.to.be.undefined;
  });

  it('should have loaded Backgrid', function() {
    expect(Backgrid).to.exist;
    expect(Backgrid).not.to.be.undefined;
  });
});
