/* global dependencies */
var $ = require('jquery');
var _ = require('underscore');
var Backbone = require('backbone');

/* local dependencies */

var Model = module.exports.Board = Backbone.Model.extend({
    'urlRoot': function() {
        return '/api/advisor/v1/campaign/' + this.get('campaignId') + '/assets/';
    }
});

