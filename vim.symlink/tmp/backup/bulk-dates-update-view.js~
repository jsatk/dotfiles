AdvisorApp.module('CampaignModule.Children', function(Module, App) {

    Module.BulkDatesUpdateView = Module.BulkUpdateView.extend({
        template: Handlebars.templates.BulkDatesUpdateForm,
        title: 'Update Dates',
        templateHelpers: function() {
            campaignStartDate: moment(this.getOption('campaignStartDate'), 'YYYY-MM-DDTHH:mm:ss').format('MM/DD/YYYY'),
            campaignEndDate:   moment(this.getOption('campaignEndDate'), 'YYYY-MM-DDTHH:mm:ss').format('MM/DD/YYYY')
        }
    });

});
