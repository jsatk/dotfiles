AdvisorApp.module('CampaignModule.Children', function(Module, App) {

    Module.BulkUpdateView = Marionette.ItemView.extend({
        className: 'bulk-update-object-form-sc',

        ui: {
            cancel: '#cancel'
        },

        triggers: {
            'click @ui.cancel': 'cancel',
            'submit form':      'save'
        }
    });

});
