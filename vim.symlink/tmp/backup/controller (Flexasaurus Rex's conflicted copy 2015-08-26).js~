/* global dependencies */
var Backbone   = require('backbone');
var _          = require('underscore');
var $          = require('jquery');
var Marionette = require('marionette');
var AdvisorApp = require('../../../../../app');

/* local dependencies */
var Base = require('../base/controller');
var View = require('./view');

/* module definition */
var Controller = Base.extend({
    onDoneOfGetBreadcrumbs: function(breadcrumbs) {
        debugger;
        this.getOption('model').set('breadcrumbs', breadcrumbs);
        var view = new View(this.options);
        this.getOption('region').show(view);
    }
});

module.exports = Controller;
